{
  "name": "@comsaurus/eslint-config-base-prettier",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint . --ext '.js'",
    "lint": "npm run eslint -- --cache --cache-location './dist/.eslintcache'",
    "lint:fix": "npm run eslint -- --fix && npm run prettier:fix",
    "prettier:fix": "prettier --ignore-path ./.gitignore --write \"./**/*.{js}\""
  },
  "dependencies": {
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
